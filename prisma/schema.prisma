generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model block_tasks {
  block_number   BigInt  @id
  block_hash     String? @db.VarChar(255)
  evm_block_hash String? @db.VarChar(255)
  status         String? @db.VarChar(255)
}
